salario = int (input('digite o valor do seu salário = '))
calculo = int
if salario <= 600 :
    print ('VOCÊ ESTÁ ISENTO!')

elif salario <= 1200 :
    calculo = salario * 0.20
    print ('seu desconto é de = ', calculo)

elif salario <= 2000 :
    calculo = salario * 0.25
    print ('seu desconto é de = ', calculo)
else:
     calculo = salario * 0.30
   
    print ('seu desconto é de = ', calculo)
